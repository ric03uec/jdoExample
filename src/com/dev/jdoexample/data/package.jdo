<?xml version="1.0"?>
<!DOCTYPE jdo PUBLIC
    "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 2.0//EN" 
    "http://java.sun.com/dtd/jdo_2_0.dtd">
<jdo>
	<package name="com.dev.jdoexample.data">
		<class name="Data" table="jdo_data">
			<field name="intData">
	            <element column="int_data"/>
        	</field>
        	<field name="stringData">
	            <element column="string_data"/>
        	</field>
        	<field name="date">
				<column name="time_stamp" jdbc-type="Date"/>
			</field>
        </class>
    </package>
 </jdo>